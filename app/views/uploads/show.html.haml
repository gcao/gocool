- content_for :head do
  - if RAILS_ENV == 'development'
    = stylesheet_link_tag 'http://localhost/jsgameviewer/view/default.css', :media => 'screen, projection'
    %script{:src => "http://ajax.googleapis.com/ajax/libs/jquery/1.2.6/jquery.min.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/js/base.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/js/model.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/js/parser.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/js/controller.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/js/weiqi_template.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/view/js/view.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/js/loader.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/js/site_handler.js", :type => "text/javascript"}
  - else
    %script{:src => "http://localhost/jsgameviewer/js/loader.js", :type => "text/javascript"}
    %script{:src => "http://localhost/jsgameviewer/js/site_handler.js", :type => "text/javascript"}
%h1=@page_header
-if flash[:success]
  .span-12
    .success= flash[:success]
.span-24
  %a.jsgv{:href => @upload.upload.url}Download SGF
