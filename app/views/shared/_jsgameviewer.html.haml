  %script(src = "/jsgameviewer/js/en_us.js" type = "text/javascript")
  %script(src = "/jsgameviewer/js/jquery-1.3.2.min.js" type = "text/javascript")
  %script(src = "/jsgameviewer/js/thickbox.js" type = "text/javascript")
  %script(src = "/jsgameviewer/js/base.js" type = "text/javascript")
  %script(src = "/jsgameviewer/js/model.js" type = "text/javascript")
