-if ENV["USE_OLD_JSGAMEVIEWER"] == 'true'
  %link(href="/jsgameviewer/view/default.css" media="screen" rel="stylesheet" type="text/css")
  %script(src = "/jsgameviewer/js/all.js" type = "text/javascript")
-else
  - if Rails.env.development?
    %link(href="/jsgameviewer/view/default.css" media="screen, projection" rel="stylesheet" type="text/css")
    %script(src = "/jsgameviewer/js/zh_cn.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/en_us.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/jquery-1.3.2.min.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/thickbox.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/base.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/model.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/parser.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/controller.js" type = "text/javascript")
    %script(src = "/jsgameviewer/view/js/view.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/updater.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/player.js" type = "text/javascript")
    %script(src = "/jsgameviewer/js/gocool.js" type = "text/javascript")
  - else
    %link(href="/jsgameviewer/view/default.css" media="screen" rel="stylesheet" type="text/css")
    = include_jquery
    %script(src = "/jsgameviewer/js/all.js" type = "text/javascript")
